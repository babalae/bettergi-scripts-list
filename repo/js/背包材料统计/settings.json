  {
    "name": "Weekly",
    "type": "checkbox",
    "label": "----------------------------------\n\n【周本素材】"
  },
  {
    "name": "Wood",
    "type": "checkbox",
    "label": "----------------------------------\n\n【木材】"
  },
  {
    "name": "CharAscension",
    "type": "checkbox",
    "label": "----------------------------------\n\n【角色突破素材】"
  },
  {
    "name": "Fishing",
    "type": "checkbox",
    "label": "----------------------------------\n\n【鱼饵、鱼类】"
  },
  {
    "name": "Gems",
    "type": "checkbox",
    "label": "----------------------------------\n\n【宝石】"
  },
  {
    "name": "Talent",
    "type": "checkbox",
    "label": "----------------------------------\n\n【角色天赋素材】"
  },
  {
    "name": "WeaponAscension",
    "type": "checkbox",
    "label": "----------------------------------\n\n【武器突破素材】"
  },
  {
    "name": "ImageDelay",
    "type": "input-text",
    "label": "数字太小可能无法识别，用?代替\n====================\n识图延迟时间（默认:10 毫秒)"
